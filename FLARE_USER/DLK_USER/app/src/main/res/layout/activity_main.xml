<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="@drawable/image_background"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/check"
        android:layout_width="200dp"
        android:layout_height="250dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/image_uncheck"
        android:fontFamily="@font/yanolja"
        android:gravity="center" />

    <Button
        android:id="@+id/go_finish"
        android:layout_width="108dp"
        android:layout_height="49dp"
        android:background="@drawable/image_btn_finish"
        android:gravity="center"

        android:visibility="gone" />

    <Button
        android:id="@+id/temp_start"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:fontFamily="@font/yanoljaegular"
        android:gravity="center"
        android:text="임시버튼(누르면 앱이 시작됩니다.)"
        android:textColor="#666666"
        android:textSize="20dp"/>
</LinearLayout>